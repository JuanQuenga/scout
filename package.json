{
  "name": "mochi",
  "description": "A versatile Chrome extension with command palette, controller testing, and multi-provider search capabilities.",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
